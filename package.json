{
  "name": "jonshort.me",
  "version": "0.1.0",
  "description": "Personal website of Jon Short - Designer",
  "homepage": "https://jonshort.me",
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/JonShort/jonshort.me"
  },
  "devDependencies": {
    "cypress": "^0.20.1",
    "imagemin": "^5.3.1",
    "imagemin-jpegtran": "^5.0.2",
    "imagemin-pngquant": "^5.0.1",
    "prop-types": "^15.5.10",
    "react-scripts": "^1.0.7",
    "tachyons": "^4.7.4",
    "tachyons-cli": "^1.0.10"
  },
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.1"
  },
  "scripts": {
    "build": "react-scripts build",
    "clean": "rm -rf build",
    "css": "tachyons src/App.css > src/index.css --minify",
    "eject": "react-scripts eject",
    "images": "node assets/imagemin/config.js",
    "prebuild": "npm run css && npm run images",
    "prestart": "npm run css && npm run images",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "test:e2e": "cypress open"
  }
}
